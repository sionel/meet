  "scripts": {
    "pod_jetify": "cd ios && pod cache clean --all && pod install && cd .. && npx jetify && npx patch-package",
    "pod_jetify_m1": "cd ios && pod cache clean --all && arch -x86_64 pod install && cd .. && npx jetify && patch-package",
    "android-bundling": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output wehago/android/app/src/main/assets/index.android.bundle --assets-dest wehago/android/app/src/main/res/",
    "android": "sudo npx react-native run-android --root wehago",
    "build-inhouse": "cd ./wehago/android && sudo rm -rf build ./app/build ./sdk/build && sudo ./gradlew assembleRelease && cd ../..",
    "build-inhouse-abb": "cd ./wehago/android && sudo rm -rf build ./app/build ./sdk/build && sudo ./gradlew bundleRelease && cd ../..",
    "set-android": "~/Library/Android/sdk/platform-tools/adb -s 2c79b31d441d7ece reverse tcp:8081 tcp:8081",
    "get-android": "~/Library/Android/sdk/platform-tools/adb devices",
    "device": "~/Library/Android/sdk/emulator/emulator @Nexus_5X_API_29",
    "clean:android": "cd ./wehago/android && sudo ./gradlew clean && cd ../..",
    "clean:ios": "cd ios && sudo xcodebuild clean && cd ../"
  },
